{
	"workbench.colorCustomizations": {
		"activityBar.background": "#167c9d",
		"activityBar.activeBackground": "#167c9d",
		"activityBar.activeBorder": "#ef8fd8",
		"activityBar.foreground": "#e7e7e7",
		"activityBar.inactiveForeground": "#e7e7e799",
		"activityBarBadge.background": "#ef8fd8",
		"activityBarBadge.foreground": "#15202b",
		"titleBar.activeBackground": "#105970",
		"titleBar.inactiveBackground": "#10597099",
		"titleBar.activeForeground": "#e7e7e7",
		"titleBar.inactiveForeground": "#e7e7e799",
		"statusBar.background": "#105970",
		"statusBarItem.hoverBackground": "#167c9d",
		"statusBar.foreground": "#e7e7e7",
		"statusBar.border": "#105970",
		"titleBar.border": "#105970"
	},
	"peacock.color": "#105970",
	"[javascript]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"[json]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features"
	},
	"[css]": {
		"editor.defaultFormatter": "stylelint.vscode-stylelint"
	},
	"[scss]": {
		"editor.defaultFormatter": "stylelint.vscode-stylelint"
	},
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll": true
	},
	"eslint.enable": true,
	"css.validate": false,
	"less.validate": false,
	"scss.validate": false,
	"files.exclude": {}
}
