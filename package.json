{
  "name": "github-sidebar",
  "version": "2.2.3",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/christianeide/github-sidebar"
  },
  "author": "Christian Sletten Eide",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development webpack",
    "version": "node ./version.js",
    "build": "NODE_ENV=production webpack -p --config webpack.production.config.js --display-error-details --colors --progress",
    "test": "jest --coverage --config config/jest.config.js --runInBand",
    "test-watch": "jest --watch --config config/jest.config.js --runInBand",
    "lint": "eslint . --fix --ext .jsx,.js --ignore-path .gitignore"
  },
  "dependencies": {
    "array-move": "^3.0.1",
    "favico.js": "^0.3.10",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-sortable-hoc": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-react-jsx": "^7.12.17",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^13.0.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^5.1.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-chrome": "^0.7.0",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "sass-loader": "^8.0.2",
    "semver": "^7.3.4",
    "style-loader": "^1.1.3",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-prettier": "^1.2.0",
    "webpack": "^4.42.1",
    "webpack-chrome-extension-reloader": "^1.3.0",
    "webpack-cli": "^3.3.11"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-react-jsx"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*.{scss,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "htmlWhitespaceSensitivity": "strict",
    "printWidth": 80,
    "singleQuote": true,
    "useTabs": true,
    "tabWidth": 2
  }
}
