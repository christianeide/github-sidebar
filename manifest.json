{
  "name": "Github Sidebar",
  "description": "",
  "author": "Christian Sletten Eide",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "https://github.com/*",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "content-script.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ]

}